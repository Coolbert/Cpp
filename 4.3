// Program wyświetlający tablicę znaków ANSI

#include <iostream>

using namespace std;

int main()
{
for (int i = 1; i < 256; i += 4)
{
cout << "| " << static_cast<char>(i) << " == " << i << " | " << static_cast<char>(i + 1) << " == " << i + 1 << " | " << static_cast<char>(i + 2) << " == " << i + 2 << " | " << static_cast<char>(i + 3) << " == " << i + 3 << " |" << endl;
}
}

// Napisz aplikację wyświetlającą listę liczb od 1 do 100 z podanymi obok
wartościami ich drugich potęg (kwadratów).

#include <iostream>

using namespace std;

int main()
{
    cout << "Tablica wyswietlajaca drugie potegi liczb: " << endl;
    for (int i=1; i<=100; i++)
    cout << i << " - " << i*i << endl;
    return 0;
}
